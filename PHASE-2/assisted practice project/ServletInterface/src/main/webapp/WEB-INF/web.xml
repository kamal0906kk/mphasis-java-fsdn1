<?xml version="1.0" encoding="UTF-8"?>
<web-app >
  <display-name>ServletInterface</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>  
  </welcome-file-list>
  <servlet>
   <servlet-name>TempServlet</servlet-name>
  <servlet-class>com.smpl.demo.TempServlet</servlet-class>
  </servlet>
  <servlet-mapping>
   <servlet-name>TempServlet</servlet-name>
   <url-pattern>/</url-pattern>
   </servlet-mapping>
    <servlet>
   <servlet-name>Generic</servlet-name>
  <servlet-class>com.smpl.demo.Generic</servlet-class>
  </servlet>
  <servlet-mapping>
   <servlet-name>Generic</servlet-name>
   <url-pattern>/Generic</url-pattern>
   </servlet-mapping>
  
</web-app>